{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    },
    "html.format.enable": false,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
}
